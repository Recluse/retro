_compact_l:
	.DB  0xFF,0x9,0xFF,0x5,0x3,0x1,0x2,0xC
	.DB  0xFF,0xA,0x8,0x6,0x4,0x25,0x10,0xFF
	.DB  0xFF,0x5D,0x4A,0xFF,0x5B,0x26,0x11,0xFF
	.DB  0xFF,0xFF,0x4B,0x3C,0x3B,0x27,0x12,0xFF
	.DB  0xFF,0x4D,0x4C,0x3D,0x28,0x14,0x13,0xFF
