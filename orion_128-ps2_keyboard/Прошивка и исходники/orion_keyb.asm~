;//---------------------------------------------------------------------------
;// Функция сжатия 512 сканкодов в 109 сканкодов
;//   Вход:  v_ext + v_a - сканкод от 0 до 511
;//   Выход: v_a - сканкод от 0 до 108
;
;void compact() {
; 0000 0181 void compact() {

	.CSEG
_compact:
; 0000 0182   if(v_ext) {
	LDS  R30,_v_ext
	CPI  R30,0
	BREQ _0x6
; 0000 0183     v_a -= 0x11;
	LDI  R30,LOW(17)
	SUB  R6,R30
; 0000 0184     if(v_a < sizeof(compact_h)) {
	LDI  R30,LOW(110)
	CP   R6,R30
	BRSH _0x7
; 0000 0185       v_a = compact_h[v_a];
	MOV  R30,R6
	LDI  R31,0
	SUBI R30,LOW(-_compact_h*2)
	SBCI R31,HIGH(-_compact_h*2)
	RJMP _0x2000004
; 0000 0186       return;
; 0000 0187     }
; 0000 0188   } else {
_0x7:
	RJMP _0x8
_0x6:
; 0000 0189     if(v_a < sizeof(compact_l)) {
	LDI  R30,LOW(132)
	CP   R6,R30
	BRSH _0x9
; 0000 018A       v_a = compact_l[v_a];
	MOV  R30,R6
	LDI  R31,0
	SUBI R30,LOW(-_compact_l*2)
	SBCI R31,HIGH(-_compact_l*2)
_0x2000004:
	LPM  R6,Z
; 0000 018B       return;
	RET
; 0000 018C     }
; 0000 018D   }
_0x9:
_0x8:
; 0000 018E   v_a = 0xFF;
	LDI  R30,LOW(255)
	MOV  R6,R30
; 0000 018F };
	RET
;
;//---------------------------------------------------------------------------
;
;void delay_msX() { while(v_i--) delay_us(10000); }
; 0000 0193 void delay_msX() { while(v_i--) delay_us(10000); }
_delay_msX:
_0xA:
	MOV  R30,R13
	DEC  R13
	CPI  R30,0
	BREQ _0xC
	__DELAY_USW 20000
	RJMP _0xA
_0xC:
	RET
;void delay_ms300() { v_i=30; delay_msX(); }
; 0000 0194 void delay_ms300() { v_i=30; delay_msX(); }
_delay_ms300:
	LDI  R30,LOW(30)
	RJMP _0x2000003
;void delay_ms800() { v_i=80; delay_msX(); }
; 0000 0195 void delay_ms800() { v_i=80; delay_msX(); }
_delay_ms800:
	LDI  R30,LOW(80)
	RJMP _0x2000003
;void delay_ms150() { v_i=15; delay_msX(); }
; 0000 0196 void delay_ms150() { v_i=15; delay_msX(); }
_delay_ms150:
	LDI  R30,LOW(15)
_0x2000003:
	MOV  R13,R30
	RCALL _delay_msX
	RET
;
;//---------------------------------------------------------------------------
;// Функция перезагрузки
;
;inline void reboot() {
; 0000 019B inline void reboot() {
_reboot:
; 0000 019C #asm
; 0000 019D   rjmp 0
  rjmp 0
; 0000 019E #endasm
; 0000 019F }
	RET
;
;//---------------------------------------------------------------------------
;// Низкоуровневые функции протокола PS/2
;
;#define PORT(X) PORT##X
;
;#define ps2_setClock0()  { PS2_PORT.PS2_CLOCK = 0; PS2_DDR.PS2_CLOCK = 1; }
;#define ps2_setClock1()  { PS2_PORT.PS2_CLOCK = 1; PS2_DDR.PS2_CLOCK = 0; }
;#define ps2_setDataOut() { PS2_DDR.PS2_DATA = 1; }
;#define ps2_setDataIn()  { PS2_DDR.PS2_DATA = 0; }
;#define ps2_setData0()   { PS2_PORT.PS2_DATA = 0; }
;#define ps2_setData1()   { PS2_PORT.PS2_DATA = 1; }
;#define ps2_waitClock0() { while(PS2_PIN.PS2_CLOCK); }
;#define ps2_waitClock1() { while(PS2_PIN.PS2_CLOCK==0); }
;#define ps2_waitClock()  { ps2_waitClock1(); ps2_waitClock0(); }
;#define ps2_data()       PS2_PIN.PS2_DATA
;#define ps2_wait()       (PS2_PIN.PS2_DATA || PS2_PIN.PS2_CLOCK)
;
;//---------------------------------------------------------------------------
;// Включить режим передачи или ожидаения.
;
;void ps2_transmitMode() {
; 0000 01B5 void ps2_transmitMode() {
_ps2_transmitMode:
; 0000 01B6   ps2_setClock0();
	CBI  0x15,1
	SBI  0x14,1
; 0000 01B7   delay_us(100);
	__DELAY_USW 200
; 0000 01B8 }
	RET
;
;//---------------------------------------------------------------------------
;// Включить режим приема. В этом режиме клаиватура может начать передачу,
;// поэтому необходимо постоянно проверять ps2_wait()
;
;void ps2_receiveMode() {
; 0000 01BE void ps2_receiveMode() {
_ps2_receiveMode:
; 0000 01BF   ps2_setClock1();
	SBI  0x15,1
	CBI  0x14,1
; 0000 01C0 }
	RET
;
;//---------------------------------------------------------------------------
;// Отправить байт клавиатуре
;//   Вход:  v_a - байт
;
;void ps2_send() {
; 0000 01C6 void ps2_send() {
_ps2_send:
; 0000 01C7   ps2_setDataOut();
	SBI  0x14,2
; 0000 01C8   ps2_setData0();
	CBI  0x15,2
; 0000 01C9   delay_us(20);
	__DELAY_USB 53
; 0000 01CA   ps2_setClock1();
	SBI  0x15,1
	CBI  0x14,1
; 0000 01CB   delay_us(10);
	__DELAY_USB 27
; 0000 01CC   v_j=0;
	CLR  R12
; 0000 01CD   ps2_waitClock0();
_0x1D:
	SBIC 0x13,1
	RJMP _0x1D
; 0000 01CE   for(v_i=0; v_i<8; v_i++) {
	CLR  R13
_0x21:
	LDI  R30,LOW(8)
	CP   R13,R30
	BRSH _0x22
; 0000 01CF     if(v_a&1) { ps2_setData1(); v_j++; } else ps2_setData0();
	SBRS R6,0
	RJMP _0x23
	SBI  0x15,2
	INC  R12
	RJMP _0x26
_0x23:
	CBI  0x15,2
_0x26:
; 0000 01D0     v_a >>= 1;
	LSR  R6
; 0000 01D1     ps2_waitClock();
_0x29:
	SBIS 0x13,1
	RJMP _0x29
_0x2C:
	SBIC 0x13,1
	RJMP _0x2C
; 0000 01D2   }
	INC  R13
	RJMP _0x21
_0x22:
; 0000 01D3   // Бит четности
; 0000 01D4   if(v_j&1) { ps2_setData0(); } else { ps2_setData1(); }
	SBRS R12,0
	RJMP _0x2F
	CBI  0x15,2
	RJMP _0x32
_0x2F:
	SBI  0x15,2
_0x32:
; 0000 01D5   ps2_waitClock();
_0x35:
	SBIS 0x13,1
	RJMP _0x35
_0x38:
	SBIC 0x13,1
	RJMP _0x38
; 0000 01D6   // Стоповый бит
; 0000 01D7   ps2_setData1();
	SBI  0x15,2
; 0000 01D8   ps2_waitClock();
_0x3D:
	SBIS 0x13,1
	RJMP _0x3D
_0x40:
	SBIC 0x13,1
	RJMP _0x40
; 0000 01D9   // Пропуск ACK
; 0000 01DA   ps2_setDataIn();
	CBI  0x14,2
; 0000 01DB   ps2_waitClock();
_0x45:
	SBIS 0x13,1
	RJMP _0x45
_0x48:
	SBIC 0x13,1
	RJMP _0x48
; 0000 01DC   if(ps2_data()) reboot(); // Ошибка клавиатуры. Перезагружаем её.
	SBIC 0x13,2
	RCALL _reboot
; 0000 01DD   ps2_waitClock1();
_0x4C:
	SBIS 0x13,1
	RJMP _0x4C
; 0000 01DE   ps2_transmitMode();
	RJMP _0x2000002
; 0000 01DF }
;
;//---------------------------------------------------------------------------
;// Принять байт от клавиатуры. Эта функция вызывается уже после того, как
;// клавиатура начала отправку
;//   Выход:  v_a - байт
;
;void ps2_recv_int() {
; 0000 01E6 void ps2_recv_int() {
_ps2_recv_int:
; 0000 01E7   v_a = 0; v_j = 0;
	CLR  R6
	CLR  R12
; 0000 01E8   for(v_i=0; v_i<8; v_i++) {
	CLR  R13
_0x50:
	LDI  R30,LOW(8)
	CP   R13,R30
	BRSH _0x51
; 0000 01E9     ps2_waitClock();
_0x52:
	SBIS 0x13,1
	RJMP _0x52
_0x55:
	SBIC 0x13,1
	RJMP _0x55
; 0000 01EA     v_a >>= 1;
	LSR  R6
; 0000 01EB     if(PS2_PIN.PS2_DATA) { v_a |= 0x80; v_j++; }
	SBIS 0x13,2
	RJMP _0x58
	LDI  R30,LOW(128)
	OR   R6,R30
	INC  R12
; 0000 01EC   }
_0x58:
	INC  R13
	RJMP _0x50
_0x51:
; 0000 01ED   // parity bit
; 0000 01EE   ps2_waitClock();
_0x59:
	SBIS 0x13,1
	RJMP _0x59
_0x5C:
	SBIC 0x13,1
	RJMP _0x5C
; 0000 01EF   if(PS2_PIN.PS2_DATA) v_j++;
	SBIC 0x13,2
	INC  R12
; 0000 01F0   // stop bit
; 0000 01F1   ps2_waitClock();
_0x60:
	SBIS 0x13,1
	RJMP _0x60
_0x63:
	SBIC 0x13,1
	RJMP _0x63
; 0000 01F2   //if((v_j&1)==0) reboot();
; 0000 01F3   //if(PS2_PIN.PS2_DATA==0) reboot();
; 0000 01F4   // end
; 0000 01F5   ps2_waitClock1();
_0x66:
	SBIS 0x13,1
	RJMP _0x66
; 0000 01F6   ps2_transmitMode();
_0x2000002:
	RCALL _ps2_transmitMode
; 0000 01F7 }
	RET
;
;//---------------------------------------------------------------------------
;// Принять байт от клавиатуры. Функция включает режим приема, ждет байт, а
;// потом выключает режим приема.
;//   Выход:  v_a - байт
;
;void ps2_recv() {
; 0000 01FE void ps2_recv() {
_ps2_recv:
; 0000 01FF   ps2_receiveMode();
	RCALL _ps2_receiveMode
; 0000 0200   while(ps2_wait());
_0x69:
	SBIC 0x13,2
	RJMP _0x6C
	SBIS 0x13,1
	RJMP _0x6B
_0x6C:
	RJMP _0x69
_0x6B:
; 0000 0201   ps2_recv_int();
	RJMP _0x2000001
; 0000 0202 }
;
;//---------------------------------------------------------------------------
;// Послать байт клавиатуре и получить от неё подтвержение приёма.
;
;void ps2_sendAndCheck() {
; 0000 0207 void ps2_sendAndCheck() {
_ps2_sendAndCheck:
; 0000 0208   ps2_send();
	RCALL _ps2_send
; 0000 0209   ps2_recv();
	RCALL _ps2_recv
; 0000 020A   if(v_a!=0xFA) reboot();
	LDI  R30,LOW(250)
	CP   R30,R6
	BREQ _0x6E
	RCALL _reboot
; 0000 020B }
_0x6E:
	RET
;
;//---------------------------------------------------------------------------
;// Сброс клавиатуры
;
;void ps2_reset() {
; 0000 0210 void ps2_reset() {
_ps2_reset:
; 0000 0211   // Аппаратный сброс.
; 0000 0212   ps2_setClock1();
	SBI  0x15,1
	CBI  0x14,1
; 0000 0213   ps2_setDataOut();
	SBI  0x14,2
; 0000 0214   ps2_setData0();
	CBI  0x15,2
; 0000 0215   delay_ms300();
	RCALL _delay_ms300
; 0000 0216   ps2_setData1();
	SBI  0x15,2
; 0000 0217   ps2_setDataIn();
	CBI  0x14,2
; 0000 0218   ps2_setClock0();
	CBI  0x15,1
	SBI  0x14,1
; 0000 0219   delay_ms800();
	RCALL _delay_ms800
; 0000 021A 
; 0000 021B   // Клавиатура исправна
; 0000 021C   ps2_recv();
	RCALL _ps2_recv
; 0000 021D   if(v_a!=0xAA) reboot();
	LDI  R30,LOW(170)
	CP   R30,R6
	BREQ _0x7F
	RCALL _reboot
; 0000 021E }
_0x7F:
	RET
;
;//---------------------------------------------------------------------------
;
;void ps2_setLeds() {
; 0000 0222 void ps2_setLeds() {
_ps2_setLeds:
; 0000 0223   v_a = 0xED; ps2_sendAndCheck();
	LDI  R30,LOW(237)
	MOV  R6,R30
	RCALL _ps2_sendAndCheck
; 0000 0224   v_a = leds; ps2_sendAndCheck();
	MOV  R6,R5
	RCALL _ps2_sendAndCheck
; 0000 0225 }
	RET
;
;//---------------------------------------------------------------------------
;//---------------------------------------------------------------------------
;//---------------------------------------------------------------------------
;// Обработка светодиодов и джойстика. Вызывается из функции ожидаение приема
;// команд от клавиатуры.
;
;void pressRelease(); // forward
;void calc(); // forward
;
;void recvIdle() {
; 0000 0230 void recvIdle() {
_recvIdle:
; 0000 0231   // Если включить эту строку, то символы дублируется. Хотя никакого эффекта от этой
; 0000 0232   // строки быть не должно.
; 0000 0233   // DECODE
; 0000 0234 
; 0000 0235   v_i=0;
	CLR  R13
; 0000 0236   if(mode!=modesCnt-1) v_i += 1; // scroll lock
	LDI  R30,LOW(1)
	CP   R30,R4
	BREQ _0x80
	INC  R13
; 0000 0237 #ifdef LED_TAPE
; 0000 0238   if(LED_PIN.LED_TAPE==0) v_i += 2; // num lock
; 0000 0239 #endif
; 0000 023A #ifdef LED_RUSLAT
; 0000 023B   if(LED_PIN.LED_RUSLAT==0) v_i += 4; // caps lock
; 0000 023C #endif
; 0000 023D 
; 0000 023E #ifdef JOY_PIN
; 0000 023F   v_j = JOY_PIN & ((1<<JOY_UP)|(1<<JOY_DOWN)|(1<<JOY_LEFT)|(1<<JOY_RIGHT)|(1<<JOY_FIRE));
; 0000 0240 #endif
; 0000 0241 
; 0000 0242   if(v_i == leds
_0x80:
; 0000 0243 #ifdef JOY_PIN
; 0000 0244     && v_j == joystic
; 0000 0245 #endif
; 0000 0246   ) return;
	CP   R5,R13
	BRNE _0x81
	RET
; 0000 0247 
; 0000 0248   // Отключаем прием
; 0000 0249   ps2_transmitMode();
_0x81:
	RCALL _ps2_transmitMode
; 0000 024A 
; 0000 024B   // Обновляем светодиоды
; 0000 024C   if(v_i != leds) {
	CP   R5,R13
	BREQ _0x82
; 0000 024D     leds = v_i;
	MOV  R5,R13
; 0000 024E     ps2_setLeds();
	RCALL _ps2_setLeds
; 0000 024F   }
; 0000 0250 #ifdef JOY_PIN
; 0000 0251   // Эмулируем нажатия кнопок джойстика
; 0000 0252   if(v_j != joystic) {
; 0000 0253     joystic = v_j;
; 0000 0254     v_j = (joystic & (1<<JOY_UP   ))==0; v_a = 104; pressRelease();
; 0000 0255     v_j = (joystic & (1<<JOY_DOWN ))==0; v_a = 105; pressRelease();
; 0000 0256     v_j = (joystic & (1<<JOY_LEFT ))==0; v_a = 106; pressRelease();
; 0000 0257     v_j = (joystic & (1<<JOY_RIGHT))==0; v_a = 107; pressRelease();
; 0000 0258     v_j = (joystic & (1<<JOY_FIRE ))==0; v_a = 108; pressRelease();
; 0000 0259     calc();
; 0000 025A   }
; 0000 025B #endif
; 0000 025C 
; 0000 025D   // Вкплючаем прием
; 0000 025E   ps2_receiveMode();
_0x82:
	RCALL _ps2_receiveMode
; 0000 025F }
	RET
;
;//---------------------------------------------------------------------------
;// Ожидаение приема команд от клавиатуры и обработка светодиодов и джойстика
;
;void ps2_recvAndIdle() {
; 0000 0264 void ps2_recvAndIdle() {
_ps2_recvAndIdle:
; 0000 0265   ps2_receiveMode();
	RCALL _ps2_receiveMode
; 0000 0266   while(ps2_wait()) {
_0x83:
	SBIC 0x13,2
	RJMP _0x86
	SBIS 0x13,1
	RJMP _0x85
_0x86:
; 0000 0267     recvIdle();
	RCALL _recvIdle
; 0000 0268   }
	RJMP _0x83
_0x85:
; 0000 0269   ps2_recv_int();
_0x2000001:
	RCALL _ps2_recv_int
; 0000 026A }
	RET
;
;//---------------------------------------------------------------------------
;// Преобразование клавиш РК86 в матрицу нажатых клавиш клаиватуры РК86
;
;void calc() {
; 0000 026F void calc() {
_calc:
; 0000 0270   // Выбираем таблицу
; 0000 0271   switch(mode) {
	MOV  R30,R4
; 0000 0272     case 0: v_decode = decodeE; break;
	CPI  R30,0
	BRNE _0x8B
	LDI  R30,LOW(_decodeE)
	LDI  R31,HIGH(_decodeE)
	RJMP _0x106
; 0000 0273     case 1: v_decode = decodeR; break;
_0x8B:
	CPI  R30,LOW(0x1)
	BRNE _0x8A
	LDI  R30,LOW(_decodeR)
	LDI  R31,HIGH(_decodeR)
_0x106:
	MOVW R8,R30
; 0000 0274 #ifdef LED_RUSLAT
; 0000 0275     case 2: v_decode = (LED_PIN&(1<<LED_RUSLAT))==0 ? decodeR : decodeE; break;
; 0000 0276 #endif
; 0000 0277   }
_0x8A:
; 0000 0278 
; 0000 0279   // Очищаем матрица 8x8
; 0000 027A   for(v_i=0; v_i<8; v_i++)
	CLR  R13
_0x8E:
	LDI  R30,LOW(8)
	CP   R13,R30
	BRSH _0x8F
; 0000 027B     v_row[v_i] = 0xFF;
	MOV  R30,R13
	LDI  R31,0
	SUBI R30,LOW(-_v_row)
	SBCI R31,HIGH(-_v_row)
	LDI  R26,LOW(255)
	STD  Z+0,R26
	INC  R13
	RJMP _0x8E
_0x8F:
; 0000 027E v_j = 0xFF;
	LDI  R30,LOW(255)
	MOV  R12,R30
; 0000 027F   for(v_i=0; v_i<pressedCnt; v_i++) {
	CLR  R13
_0x91:
	CP   R13,R7
	BRSH _0x92
; 0000 0280     v_u = v_decode[pressed[v_i]];
	MOV  R30,R13
	LDI  R31,0
	SUBI R30,LOW(-_pressed)
	SBCI R31,HIGH(-_pressed)
	LD   R30,Z
	MOVW R26,R8
	LDI  R31,0
	LSL  R30
	ROL  R31
	ADD  R26,R30
	ADC  R27,R31
	LD   R10,X+
	LD   R11,X
; 0000 0281     if(v_u & 0x40) v_row[v_u&7] &= ~(1<<((v_u>>3)&7));
	SBRS R10,6
	RJMP _0x93
	MOVW R30,R10
	ANDI R30,LOW(0x7)
	ANDI R31,HIGH(0x7)
	SUBI R30,LOW(-_v_row)
	SBCI R31,HIGH(-_v_row)
	MOVW R22,R30
	LD   R1,Z
	MOVW R30,R10
	RCALL __LSRW3
	ANDI R30,LOW(0x7)
	LDI  R26,LOW(1)
	RCALL __LSLB12
	COM  R30
	AND  R30,R1
	MOVW R26,R22
	ST   X,R30
; 0000 0282     if(v_u & K_RUSLAT) v_j &= ~(1<<OUT_RUSLAT);
_0x93:
	SBRS R11,2
	RJMP _0x94
	LDI  R30,LOW(223)
	AND  R12,R30
; 0000 0283     if(v_u & K_SS    ) v_j &= ~(1<<OUT_SS);
_0x94:
	SBRS R11,0
	RJMP _0x95
	LDI  R30,LOW(239)
	AND  R12,R30
; 0000 0284     if(v_u & K_US    ) v_j &= ~(1<<OUT_US);
_0x95:
	SBRS R11,1
	RJMP _0x96
	LDI  R30,LOW(247)
	AND  R12,R30
; 0000 0285 #ifdef OUT_RESET
; 0000 0286     if(v_u & K_RESET ) v_j &= ~(1<<OUT_RESET);
; 0000 0287 #endif
; 0000 0288   }
_0x96:
	INC  R13
	RJMP _0x91
_0x92:
; 0000 0289 
; 0000 028A   // Записываем шифты
; 0000 028B   v_i = OUT_PORT;
	IN   R13,21
; 0000 028C   OUT_PORT = v_j;
	OUT  0x15,R12
; 0000 028D 
; 0000 028E   // Если шифты изменились, то делаем паузу в 20 мс перед загрузкой кнопок.
; 0000 028F   if(v_i != v_j) delay_us(20000);
	CP   R12,R13
	BREQ _0x97
	__DELAY_USW 40000
; 0000 0290 
; 0000 0291   // Преобразовываем матрицу 8x8 в 256x8
; 0000 0292   v_i = 0;
_0x97:
	CLR  R13
; 0000 0293   do {
_0x99:
; 0000 0294     v_j = 0xFF;
	LDI  R30,LOW(255)
	MOV  R12,R30
; 0000 0295     if(0==(v_i&0x01)) v_j &= v_row[0];
	SBRC R13,0
	RJMP _0x9B
	LDS  R30,_v_row
	AND  R12,R30
; 0000 0296     if(0==(v_i&0x02)) v_j &= v_row[1];
_0x9B:
	SBRC R13,1
	RJMP _0x9C
	__GETB1MN _v_row,1
	AND  R12,R30
; 0000 0297     if(0==(v_i&0x04)) v_j &= v_row[2];
_0x9C:
	SBRC R13,2
	RJMP _0x9D
	__GETB1MN _v_row,2
	AND  R12,R30
; 0000 0298     if(0==(v_i&0x08)) v_j &= v_row[3];
_0x9D:
	SBRC R13,3
	RJMP _0x9E
	__GETB1MN _v_row,3
	AND  R12,R30
; 0000 0299     if(0==(v_i&0x10)) v_j &= v_row[4];
_0x9E:
	SBRC R13,4
	RJMP _0x9F
	__GETB1MN _v_row,4
	AND  R12,R30
; 0000 029A     if(0==(v_i&0x20)) v_j &= v_row[5];
_0x9F:
	SBRC R13,5
	RJMP _0xA0
	__GETB1MN _v_row,5
	AND  R12,R30
; 0000 029B     if(0==(v_i&0x40)) v_j &= v_row[6];
_0xA0:
	SBRC R13,6
	RJMP _0xA1
	__GETB1MN _v_row,6
	AND  R12,R30
; 0000 029C     if(0==(v_i&0x80)) v_j &= v_row[7];
_0xA1:
	SBRC R13,7
	RJMP _0xA2
	__GETB1MN _v_row,7
	AND  R12,R30
; 0000 029D     data[v_i] = v_j;
_0xA2:
	MOV  R30,R13
	LDI  R31,0
	SUBI R30,LOW(-_data)
	SBCI R31,HIGH(-_data)
	ST   Z,R12
; 0000 029E     v_i++;
	INC  R13
; 0000 029F   } while(v_i!=0);
	TST  R13
	BRNE _0x99
; 0000 02A0 
; 0000 02A1   // Обновляем данные на выходе МК
; 0000 02A2   DECODE
	IN   R30,0x10
	ANDI R30,0xFB
	MOV  R26,R30
	SBIS 0x13,0
	RJMP _0xA3
	LDI  R30,LOW(4)
	LDI  R31,HIGH(4)
	RJMP _0xA4
_0xA3:
	LDI  R30,LOW(0)
	LDI  R31,HIGH(0)
_0xA4:
	OR   R30,R26
	LDI  R31,0
	LDI  R31,0
	SUBI R30,LOW(-_data)
	SBCI R31,HIGH(-_data)
	LD   R30,Z
	OUT  0x18,R30
; 0000 02A3 }
	RET
;
;//---------------------------------------------------------------------------
;// Нажата кнопка переключения раскладок
;
;void pressModeKey() {
; 0000 02A8 void pressModeKey() {
_pressModeKey:
; 0000 02A9   mode++;
	INC  R4
; 0000 02AA   if(mode>=modesCnt) mode=0;
	MOV  R26,R4
	CLR  R27
	SBIW R26,2
	BRLT _0xA6
	CLR  R4
; 0000 02AB }
_0xA6:
	RET
;
;//---------------------------------------------------------------------------
;// Нажатие или отпускание кнопки
;// Использует v_i, v_j
;
;void pressRelease() {
; 0000 02B1 void pressRelease() {
_pressRelease:
; 0000 02B2   if(v_j) {
	TST  R12
	BREQ _0xA7
; 0000 02B3     // Нажатие
; 0000 02B4     if(pressedCnt == pressedMax) return;
	LDI  R30,LOW(16)
	CP   R30,R7
	BRNE _0xA8
	RET
; 0000 02B5     for(v_i=0; v_i<pressedCnt; v_i++)
_0xA8:
	CLR  R13
_0xAA:
	CP   R13,R7
	BRSH _0xAB
; 0000 02B6       if(pressed[v_i]==v_a)
	MOV  R30,R13
	LDI  R31,0
	SUBI R30,LOW(-_pressed)
	SBCI R31,HIGH(-_pressed)
	LD   R26,Z
	CP   R6,R26
	BRNE _0xAC
; 0000 02B7         return;
	RET
; 0000 02B8     pressed[pressedCnt++] = v_a;
_0xAC:
	INC  R13
	RJMP _0xAA
_0xAB:
	MOV  R30,R7
	INC  R7
	LDI  R31,0
	SUBI R30,LOW(-_pressed)
	SBCI R31,HIGH(-_pressed)
	ST   Z,R6
; 0000 02B9     if(v_decode[v_a] & K_MODE) pressModeKey();
	MOV  R30,R6
	MOVW R26,R8
	LDI  R31,0
	LSL  R30
	ROL  R31
	ADD  R26,R30
	ADC  R27,R31
	RCALL __GETW1P
	ANDI R31,HIGH(0x1000)
	BREQ _0xAD
	RCALL _pressModeKey
; 0000 02BA   } else {
_0xAD:
	RJMP _0xAE
_0xA7:
; 0000 02BB     // Отпускание
; 0000 02BC     for(v_i=0; v_i<pressedCnt; v_i++)
	CLR  R13
_0xB0:
	CP   R13,R7
	BRSH _0xB1
; 0000 02BD       if(pressed[v_i]==v_a) {
	MOV  R30,R13
	LDI  R31,0
	SUBI R30,LOW(-_pressed)
	SBCI R31,HIGH(-_pressed)
	LD   R26,Z
	CP   R6,R26
	BRNE _0xB2
; 0000 02BE         pressedCnt--;
	DEC  R7
; 0000 02BF         for(;v_i<pressedCnt; v_i++)
_0xB4:
	CP   R13,R7
	BRSH _0xB5
; 0000 02C0           pressed[v_i] = pressed[v_i+1];
	MOV  R26,R13
	LDI  R27,0
	SUBI R26,LOW(-_pressed)
	SBCI R27,HIGH(-_pressed)
	MOV  R30,R13
	LDI  R31,0
	__ADDW1MN _pressed,1
	LD   R30,Z
	ST   X,R30
	INC  R13
	RJMP _0xB4
_0xB5:
; 0000 02C1 return;
	RET
; 0000 02C2       }
; 0000 02C3   }
_0xB2:
	INC  R13
	RJMP _0xB0
_0xB1:
_0xAE:
; 0000 02C4 }
	RET
;
;//---------------------------------------------------------------------------
;
;void readEEPROM () {
; 0000 02C8 void readEEPROM () {
_readEEPROM:
; 0000 02C9   while (EECR.1 != 0);
_0xB6:
	SBIC 0x1C,1
	RJMP _0xB6
; 0000 02CA   EEAR = v_u; v_u++;
	__OUTWR 10,11,30
	MOVW R30,R10
	ADIW R30,1
	MOVW R10,R30
; 0000 02CB   EECR.0 = 1;
	SBI  0x1C,0
; 0000 02CC   EECR.0 = 0;
	CBI  0x1C,0
; 0000 02CD }
	RET
;
;//---------------------------------------------------------------------------
;
;void writeEEPROM() {
; 0000 02D1 void writeEEPROM() {
_writeEEPROM:
; 0000 02D2   while(EECR.1 != 0);
_0xBD:
	SBIC 0x1C,1
	RJMP _0xBD
; 0000 02D3   EEDR = v_a;
	OUT  0x1D,R6
; 0000 02D4   EEAR = v_u; v_u++;
	__OUTWR 10,11,30
	MOVW R30,R10
	ADIW R30,1
	MOVW R10,R30
; 0000 02D5   EECR.2 = 1;
	SBI  0x1C,2
; 0000 02D6   EECR.1 = 1;
	SBI  0x1C,1
; 0000 02D7 }
	RET
;
;//---------------------------------------------------------------------------
;
;void loadDecode() {
; 0000 02DB void loadDecode() {
_loadDecode:
; 0000 02DC   v_u = 0;
	CLR  R10
	CLR  R11
; 0000 02DD   for(v_i=0; v_i<decodeSize*2; v_i++) {
	CLR  R13
_0xC5:
	LDI  R30,LOW(218)
	CP   R13,R30
	BRSH _0xC6
; 0000 02DE     readEEPROM();
	RCALL _readEEPROM
; 0000 02DF     ((unsigned char*)decodeE)[v_i] = EEDR;
	MOV  R26,R13
	LDI  R27,0
	SUBI R26,LOW(-_decodeE)
	SBCI R27,HIGH(-_decodeE)
	IN   R30,0x1D
	ST   X,R30
; 0000 02E0   }
	INC  R13
	RJMP _0xC5
_0xC6:
; 0000 02E1   for(v_i=0; v_i<decodeSize*2; v_i++) {
	CLR  R13
_0xC8:
	LDI  R30,LOW(218)
	CP   R13,R30
	BRSH _0xC9
; 0000 02E2     readEEPROM();
	RCALL _readEEPROM
; 0000 02E3     ((unsigned char*)decodeR)[v_i] = EEDR;
	MOV  R26,R13
	LDI  R27,0
	SUBI R26,LOW(-_decodeR)
	SBCI R27,HIGH(-_decodeR)
	IN   R30,0x1D
	ST   X,R30
; 0000 02E4   }
	INC  R13
	RJMP _0xC8
_0xC9:
; 0000 02E5   readEEPROM();
	RCALL _readEEPROM
; 0000 02E6   mode = EEDR;
	IN   R4,29
; 0000 02E7   if(mode >= modesCnt) mode = 0;
	MOV  R26,R4
	CLR  R27
	SBIW R26,2
	BRLT _0xCA
	CLR  R4
; 0000 02E8 }
_0xCA:
	RET
;
;//---------------------------------------------------------------------------
;
;void saveDecode() {
; 0000 02EC void saveDecode() {
_saveDecode:
; 0000 02ED   v_u = 0;
	CLR  R10
	CLR  R11
; 0000 02EE   for(v_i=0; v_i<decodeSize*2; v_i++) {
	CLR  R13
_0xCC:
	LDI  R30,LOW(218)
	CP   R13,R30
	BRSH _0xCD
; 0000 02EF     v_a = ((unsigned char*)decodeE)[v_i];
	MOV  R30,R13
	LDI  R31,0
	SUBI R30,LOW(-_decodeE)
	SBCI R31,HIGH(-_decodeE)
	LD   R6,Z
; 0000 02F0     writeEEPROM();
	RCALL _writeEEPROM
; 0000 02F1   }
	INC  R13
	RJMP _0xCC
_0xCD:
; 0000 02F2   for(v_i=0; v_i<decodeSize*2; v_i++) {
	CLR  R13
_0xCF:
	LDI  R30,LOW(218)
	CP   R13,R30
	BRSH _0xD0
; 0000 02F3     v_a = ((unsigned char*)decodeR)[v_i];
	MOV  R30,R13
	LDI  R31,0
	SUBI R30,LOW(-_decodeR)
	SBCI R31,HIGH(-_decodeR)
	LD   R6,Z
; 0000 02F4     writeEEPROM();
	RCALL _writeEEPROM
; 0000 02F5   }
	INC  R13
	RJMP _0xCF
_0xD0:
; 0000 02F6   v_a = mode;
	MOV  R6,R4
; 0000 02F7   writeEEPROM();
	RCALL _writeEEPROM
; 0000 02F8 }
	RET
;
;//---------------------------------------------------------------------------
;
;void resetData() {
; 0000 02FC void resetData() {
_resetData:
; 0000 02FD   v_i=0;
	CLR  R13
; 0000 02FE   for(;;) {
_0xD2:
; 0000 02FF     data[v_i] = 255;
	MOV  R30,R13
	LDI  R31,0
	SUBI R30,LOW(-_data)
	SBCI R31,HIGH(-_data)
	LDI  R26,LOW(255)
	STD  Z+0,R26
; 0000 0300     v_i++;
	INC  R13
; 0000 0301     if(v_i==0) break;
	TST  R13
	BRNE _0xD2
; 0000 0302   }
; 0000 0303 }
	RET
;
;//---------------------------------------------------------------------------
;
;void resetDecode() {
; 0000 0307 void resetDecode() {
_resetDecode:
; 0000 0308   for(v_i=0; v_i<decodeSize; v_i++) {
	CLR  R13
_0xD6:
	MOV  R26,R13
	CLR  R27
	CPI  R26,LOW(0x6D)
	LDI  R30,HIGH(0x6D)
	CPC  R27,R30
	BRGE _0xD7
; 0000 0309     decodeR[v_i] = decodeR_std[v_i];
	MOV  R30,R13
	LDI  R26,LOW(_decodeR)
	LDI  R27,HIGH(_decodeR)
	LDI  R31,0
	LSL  R30
	ROL  R31
	ADD  R30,R26
	ADC  R31,R27
	MOVW R22,R30
	MOV  R30,R13
	LDI  R26,LOW(_decodeR_std*2)
	LDI  R27,HIGH(_decodeR_std*2)
	LDI  R31,0
	LSL  R30
	ROL  R31
	ADD  R30,R26
	ADC  R31,R27
	RCALL __GETW1PF
	MOVW R26,R22
	ST   X+,R30
	ST   X,R31
; 0000 030A     decodeE[v_i] = decodeE_std[v_i];
	MOV  R30,R13
	LDI  R26,LOW(_decodeE)
	LDI  R27,HIGH(_decodeE)
	LDI  R31,0
	LSL  R30
	ROL  R31
	ADD  R30,R26
	ADC  R31,R27
	MOVW R22,R30
	MOV  R30,R13
	LDI  R26,LOW(_decodeE_std*2)
	LDI  R27,HIGH(_decodeE_std*2)
	LDI  R31,0
	LSL  R30
	ROL  R31
	ADD  R30,R26
	ADC  R31,R27
	RCALL __GETW1PF
	MOVW R26,R22
	ST   X+,R30
	ST   X,R31
; 0000 030B   }
	INC  R13
	RJMP _0xD6
_0xD7:
; 0000 030C }
	RET
;
;//---------------------------------------------------------------------------
;
;void demo() {
; 0000 0310 void demo() {
_demo:
; 0000 0311   leds=1; ps2_setLeds(); delay_ms150();
	LDI  R30,LOW(1)
	MOV  R5,R30
	RCALL _ps2_setLeds
	RCALL _delay_ms150
; 0000 0312   leds=4; ps2_setLeds(); delay_ms150();
	LDI  R30,LOW(4)
	MOV  R5,R30
	RCALL _ps2_setLeds
	RCALL _delay_ms150
; 0000 0313   leds=2; ps2_setLeds(); delay_ms150();
	LDI  R30,LOW(2)
	MOV  R5,R30
	RCALL _ps2_setLeds
	RCALL _delay_ms150
; 0000 0314 }
	RET
;
;//---------------------------------------------------------------------------
;
;void programmMode() {
; 0000 0318 void programmMode() {
_programmMode:
; 0000 0319   if(pressedCnt!=1) return;
	LDI  R30,LOW(1)
	CP   R30,R7
	BREQ _0xD8
	RET
; 0000 031A 
; 0000 031B   resetData();
_0xD8:
	RCALL _resetData
; 0000 031C 
; 0000 031D   demo();
	RCALL _demo
; 0000 031E   leds=7; ps2_setLeds();
	LDI  R30,LOW(7)
	MOV  R5,R30
	RCALL _ps2_setLeds
; 0000 031F 
; 0000 0320   v_u = 0;
	CLR  R10
	CLR  R11
; 0000 0321   for(;;) {
_0xDA:
; 0000 0322     ps2_recv();
	RCALL _ps2_recv
; 0000 0323     if(v_a==0xE0) ps2_recv();
	LDI  R30,LOW(224)
	CP   R30,R6
	BRNE _0xDC
	RCALL _ps2_recv
; 0000 0324     if(v_a==0xF0) {
_0xDC:
	LDI  R30,LOW(240)
	CP   R30,R6
	BRNE _0xDD
; 0000 0325       ps2_recv();
	RCALL _ps2_recv
; 0000 0326       continue;
	RJMP _0xD9
; 0000 0327     }
; 0000 0328     switch(v_a) {
_0xDD:
	MOV  R30,R6
; 0000 0329       case 0x45: v_a=0; break;
	CPI  R30,LOW(0x45)
	BRNE _0xE1
	CLR  R6
	RJMP _0xE0
; 0000 032A       case 0x16: v_a=1; break;
_0xE1:
	CPI  R30,LOW(0x16)
	BRNE _0xE2
	LDI  R30,LOW(1)
	MOV  R6,R30
	RJMP _0xE0
; 0000 032B       case 0x1E: v_a=2; break;
_0xE2:
	CPI  R30,LOW(0x1E)
	BRNE _0xE3
	LDI  R30,LOW(2)
	MOV  R6,R30
	RJMP _0xE0
; 0000 032C       case 0x26: v_a=3; break;
_0xE3:
	CPI  R30,LOW(0x26)
	BRNE _0xE4
	LDI  R30,LOW(3)
	MOV  R6,R30
	RJMP _0xE0
; 0000 032D       case 0x25: v_a=4; break;
_0xE4:
	CPI  R30,LOW(0x25)
	BRNE _0xE5
	LDI  R30,LOW(4)
	MOV  R6,R30
	RJMP _0xE0
; 0000 032E       case 0x2E: v_a=5; break;
_0xE5:
	CPI  R30,LOW(0x2E)
	BRNE _0xE6
	LDI  R30,LOW(5)
	MOV  R6,R30
	RJMP _0xE0
; 0000 032F       case 0x36: v_a=6; break;
_0xE6:
	CPI  R30,LOW(0x36)
	BRNE _0xE7
	LDI  R30,LOW(6)
	MOV  R6,R30
	RJMP _0xE0
; 0000 0330       case 0x3D: v_a=7; break;
_0xE7:
	CPI  R30,LOW(0x3D)
	BRNE _0xE8
	LDI  R30,LOW(7)
	MOV  R6,R30
	RJMP _0xE0
; 0000 0331       case 0x3E: v_a=8; break;
_0xE8:
	CPI  R30,LOW(0x3E)
	BRNE _0xE9
	LDI  R30,LOW(8)
	MOV  R6,R30
	RJMP _0xE0
; 0000 0332       case 0x46: v_a=9; break;
_0xE9:
	CPI  R30,LOW(0x46)
	BRNE _0xEA
	LDI  R30,LOW(9)
	MOV  R6,R30
	RJMP _0xE0
; 0000 0333       case 0x5A: goto ok;
_0xEA:
	CPI  R30,LOW(0x5A)
	BREQ _0xEC
; 0000 0334       default: goto exit;
	RJMP _0xEE
; 0000 0335     }
_0xE0:
; 0000 0336     v_u *= 10;
	MOVW R30,R10
	LDI  R26,LOW(10)
	LDI  R27,HIGH(10)
	RCALL __MULW12U
	MOVW R10,R30
; 0000 0337     v_u += v_a;
	MOV  R30,R6
	LDI  R31,0
	__ADDWRR 10,11,30,31
; 0000 0338   }
_0xD9:
	RJMP _0xDA
; 0000 0339 ok:
_0xEC:
; 0000 033A   switch(v_u) {
	MOVW R30,R10
; 0000 033B     case 65534: resetDecode();
	CPI  R30,LOW(0xFFFE)
	LDI  R26,HIGH(0xFFFE)
	CPC  R31,R26
	BRNE _0xF2
	RCALL _resetDecode
; 0000 033C     case 65535: saveDecode(); break;
	RJMP _0xF3
_0xF2:
	CPI  R30,LOW(0xFFFF)
	LDI  R26,HIGH(0xFFFF)
	CPC  R31,R26
	BRNE _0xF5
_0xF3:
	RCALL _saveDecode
	RJMP _0xF1
; 0000 033D     default: v_decode[pressed[0]] = v_u; break;
_0xF5:
	LDS  R30,_pressed
	MOVW R26,R8
	LDI  R31,0
	LSL  R30
	ROL  R31
	ADD  R30,R26
	ADC  R31,R27
	ST   Z,R10
	STD  Z+1,R11
; 0000 033E   }
_0xF1:
; 0000 033F 
; 0000 0340 exit:
_0xEE:
; 0000 0341   demo();
	RCALL _demo
; 0000 0342 
; 0000 0343   pressedCnt=0;
	CLR  R7
; 0000 0344 }
	RET
;
;//---------------------------------------------------------------------------
;
;void main(void) {
; 0000 0348 void main(void) {
_main:
; 0000 0349 #ifdef ATMEGA16
; 0000 034A   PORTA = 0xFF;
; 0000 034B #endif
; 0000 034C   PORTB = 0xFF;
	LDI  R30,LOW(255)
	OUT  0x18,R30
; 0000 034D   PORTC = 0xFF;
	OUT  0x15,R30
; 0000 034E   PORTD = 0xFF;
	OUT  0x12,R30
; 0000 034F 
; 0000 0350   // Настройка портов
; 0000 0351 #ifdef OUT_RESET
; 0000 0352   OUT_DDR  |= (1<<OUT_RESET);
; 0000 0353 #endif
; 0000 0354   OUT_DDR  |= (1<<OUT_RUSLAT)|(1<<OUT_SS)|(1<<OUT_US);
	IN   R30,0x14
	ORI  R30,LOW(0x38)
	OUT  0x14,R30
; 0000 0355   DECODE_DDR = 0xFF;
	LDI  R30,LOW(255)
	OUT  0x17,R30
; 0000 0356 
; 0000 0357   // Сброс переменных
; 0000 0358   leds       = 0;
	CLR  R5
; 0000 0359   mode       = modesCnt-1;
	LDI  R30,LOW(1)
	MOV  R4,R30
; 0000 035A   pressedCnt = 0;
	CLR  R7
; 0000 035B   v_decode   = decodeE;
	LDI  R30,LOW(_decodeE)
	LDI  R31,HIGH(_decodeE)
	MOVW R8,R30
; 0000 035C 
; 0000 035D   resetData();
	RCALL _resetData
; 0000 035E   loadDecode();
	RCALL _loadDecode
; 0000 035F 
; 0000 0360   // Сброс клавиатуры
; 0000 0361   ps2_reset();
	RCALL _ps2_reset
; 0000 0362 
; 0000 0363   // Вкдючаем внешнее прерывание
; 0000 0364 #ifdef ATMEGA48
; 0000 0365   ??? // Включить прерывание
; 0000 0366 #else
; 0000 0367   #define ISC00 0
; 0000 0368   #define ISC01 1
; 0000 0369   #define INT0  6
; 0000 036A   MCUCR |= (1<<ISC01)|(0<<ISC00); // falling
	IN   R30,0x35
	ORI  R30,2
	OUT  0x35,R30
; 0000 036B   GICR |= (1<<INT0);
	IN   R30,0x3B
	ORI  R30,0x40
	OUT  0x3B,R30
; 0000 036C #endif
; 0000 036D 
; 0000 036E   #asm("sei")
	sei
; 0000 036F 
; 0000 0370   // Основной цикл
; 0000 0371   for(;;) {
_0xF7:
; 0000 0372     ps2_recvAndIdle();
	RCALL _ps2_recvAndIdle
; 0000 0373 
; 0000 0374     // Обработка кнопки Pause
; 0000 0375     if(v_a==0xE1) {
	LDI  R30,LOW(225)
	CP   R30,R6
	BRNE _0xF9
; 0000 0376       // Вот такой скан код у этой кнопки
; 0000 0377       ps2_recvAndIdle(); if(v_a!=0x14) reboot();
	RCALL _ps2_recvAndIdle
	LDI  R30,LOW(20)
	CP   R30,R6
	BREQ _0xFA
	RCALL _reboot
; 0000 0378       ps2_recvAndIdle(); if(v_a!=0x77) reboot();
_0xFA:
	RCALL _ps2_recvAndIdle
	LDI  R30,LOW(119)
	CP   R30,R6
	BREQ _0xFB
	RCALL _reboot
; 0000 0379       ps2_recvAndIdle(); if(v_a!=0xE1) reboot();
_0xFB:
	RCALL _ps2_recvAndIdle
	LDI  R30,LOW(225)
	CP   R30,R6
	BREQ _0xFC
	RCALL _reboot
; 0000 037A       ps2_recvAndIdle(); if(v_a!=0xF0) reboot();
_0xFC:
	RCALL _ps2_recvAndIdle
	LDI  R30,LOW(240)
	CP   R30,R6
	BREQ _0xFD
	RCALL _reboot
; 0000 037B       ps2_recvAndIdle(); if(v_a!=0x14) reboot();
_0xFD:
	RCALL _ps2_recvAndIdle
	LDI  R30,LOW(20)
	CP   R30,R6
	BREQ _0xFE
	RCALL _reboot
; 0000 037C       ps2_recvAndIdle(); if(v_a!=0xF0) reboot();
_0xFE:
	RCALL _ps2_recvAndIdle
	LDI  R30,LOW(240)
	CP   R30,R6
	BREQ _0xFF
	RCALL _reboot
; 0000 037D       ps2_recvAndIdle(); if(v_a!=0x77) reboot();
_0xFF:
	RCALL _ps2_recvAndIdle
	LDI  R30,LOW(119)
	CP   R30,R6
	BREQ _0x100
	RCALL _reboot
; 0000 037E 
; 0000 037F       programmMode();
_0x100:
	RCALL _programmMode
; 0000 0380       continue;
	RJMP _0xF6
; 0000 0381     }
; 0000 0382 
; 0000 0383     v_ext = 0;
_0xF9:
	LDI  R30,LOW(0)
	STS  _v_ext,R30
; 0000 0384     if(v_a==0xE0) { v_ext=1; ps2_recvAndIdle(); }
	LDI  R30,LOW(224)
	CP   R30,R6
	BRNE _0x101
	LDI  R30,LOW(1)
	STS  _v_ext,R30
	RCALL _ps2_recvAndIdle
; 0000 0385 
; 0000 0386     if(v_a==0xF0) {
_0x101:
	LDI  R30,LOW(240)
	CP   R30,R6
	BRNE _0x102
; 0000 0387       ps2_recvAndIdle(); // Эта функция портит все переменные кроме v_ext
	RCALL _ps2_recvAndIdle
; 0000 0388       v_j = 0;
	CLR  R12
; 0000 0389     } else {
	RJMP _0x103
_0x102:
; 0000 038A       v_j = 1;
	LDI  R30,LOW(1)
	MOV  R12,R30
; 0000 038B     }
_0x103:
; 0000 038C     compact();
	RCALL _compact
; 0000 038D     if(v_a!=0xFF) {
	LDI  R30,LOW(255)
	CP   R30,R6
	BREQ _0x104
; 0000 038E       pressRelease();
	RCALL _pressRelease
; 0000 038F       calc();
	RCALL _calc
; 0000 0390     }
; 0000 0391   }
_0x104:
_0xF6:
	RJMP _0xF7
; 0000 0392 }
_0x105:
	RJMP _0x105
;
;//interrupt [EXT_INT0] void ext_int0() {
;//  DECODE
;//}

	.DSEG
_data:
	.BYTE 0x100
_decodeE:
	.BYTE 0xDA
_decodeR:
	.BYTE 0xDA
_pressed:
	.BYTE 0x10
_v_ext:
	.BYTE 0x1
_v_row:
	.BYTE 0x8

	.CSEG

	.CSEG
__LSLB12:
	TST  R30
	MOV  R0,R30
	MOV  R30,R26
	BREQ __LSLB12R
__LSLB12L:
	LSL  R30
	DEC  R0
	BRNE __LSLB12L
__LSLB12R:
	RET

__LSRW3:
	LSR  R31
	ROR  R30
__LSRW2:
	LSR  R31
	ROR  R30
	LSR  R31
	ROR  R30
	RET

__MULW12U:
	MUL  R31,R26
	MOV  R31,R0
	MUL  R30,R27
	ADD  R31,R0
	MUL  R30,R26
	MOV  R30,R0
	ADD  R31,R1
	RET

__GETW1P:
	LD   R30,X+
	LD   R31,X
	SBIW R26,1
	RET

__GETW1PF:
	LPM  R0,Z+
	LPM  R31,Z
	MOV  R30,R0
	RET

;END OF CODE MARKER
__END_OF_CODE:
